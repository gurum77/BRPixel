[application]

config/name="BRPixel"
run/main_scene="res://Main.tscn"
boot_splash/image="res://Assets/icon_main_128.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"
config/windows_native_icon="res://icon_main.ico"

[autoload]

StaticData="*res://StaticData.gd"
GeometryMaker="*res://Tools/GeometryMaker.gd"
NodeManager="*res://NodeManager.gd"
InputManager="*res://InputManager.gd"
Define="*res://Define.gd"
Util="*res://Util.gd"
ImageHelper="*res://ImageHelper.gd"
UndoManager="*res://Tools/UndoManager.gd"
FileManager="*res://FileManager.gd"
PluginManager="*res://PluginManager.gd"

[display]

window/size/width=800
window/size/test_width=1280
window/size/test_height=800
window/dpi/allow_hidpi=true
window/handheld/orientation="sensor_landscape"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[gui]

theme/custom_font="res://Fonts/ui_normal.tres"

